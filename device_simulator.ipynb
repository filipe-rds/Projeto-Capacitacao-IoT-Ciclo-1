        "\"\"\"\n",
        "import argparse\n",
        "\n",
        "# Construct the argument parser\n",
        "ap = argparse.ArgumentParser()\n",
        "\n",
        "# Add the arguments to the parser\n",
        "ap.add_argument(\"-a\", \"--address\", required=True,\n",
        "   help=\"Server IP Address or URL\")\n",
        "ap.add_argument(\"-p\", \"--port\", required=True,\n",
        "   help=\"Server Port\")\n",
        "ap.add_argument(\"-d\", \"--deviceid\", required=True,\n",
        "   help=\"Device ID\")\n",
        "\n",
        "args = vars(ap.parse_args())\n",
        "\n",
        "\n",
        "HOST = args['address']\n",
        "PORT = int(args['port'])\n",
        "device_id = args['deviceid']\n",
        "\"\"\"\n",
        "\n",
        "import socket\n",
        "import time\n",
        "import numpy as np\n",
        "\n",
        "\n",
        "HOST = 'playback.laced.com.br'\n",
        "PORT = 50000\n",
        "device_id = '20222370009'\n",
        "\n",
        "# Criar um socket\n",
        "sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
        "\n",
        "# Conectar-se ao host e ﾃ porta remotos\n",
        "sock.connect((HOST, PORT))\n",
        "\n",
        "print(\"Conectado\")\n",
        "\n",
        "# Enviar uma solicitaﾃｧﾃ｣o ao host\n",
        "sock.send((device_id + \"\\n\").encode()[:-1])\n",
        "\n",
        "# Obter a resposta do host, no mﾃ｡ximo, digamos, 1.024 bytes\n",
        "response_data = sock.recv(1024)\n",
        "\n",
        "sresponse = response_data.decode(\"utf-8\")\n",
        "print(sresponse)\n",
        "\n",
        "if sresponse == \"fail\":\n",
        "    sock.close()\n",
        "    exit()\n",
        "\n",
        "# Enviar valores randﾃｴmicos com ajuda da biblioteca Numpy\n",
        "for i in range(100):\n",
        "    temp = np.random.uniform(15, 32)\n",
        "    temp = round(temp,2)\n",
        "\n",
        "    print(f\"Dado Enviado: {temp:.2f} 邃ソ")\n",
        "    sock.send((str(temp) + \"\\n\").encode()[:-1])\n",
        "    time.sleep(1)\n",
        "\n",
        "sock.send((\"alive\" + \"\\n\").encode()[:-1])\n",
        "\n",
        "# Obter a resposta do host, no mﾃ｡ximo, digamos, 1.024 bytes\n",
        "response_data = sock.recv(1024)\n",
        "\n",
        "sresponse = response_data.decode(\"utf-8\")\n",
        "print(sresponse)\n",
        "\n",
        "if sresponse == \"fail\":\n",
        "    sock.close()\n",
        "    exit()\n",
        "\n",
        "# Encerrar\n",
        "sock.close()"
      ]
